{
  "scripts": {
    "start": "tsx watch main.ts",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "tsc": "^2.0.4",
    "tsx": "^4.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "main.(t|j)s"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.3"
  }
}
